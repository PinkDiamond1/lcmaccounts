<%= osu.block_to_partial 'shared/modal',
                         html_id: 'dev_console_dialog',
                         width: '900px',
                         height: '500px' do %>

  <div class="modal-header" style="border-bottom: none">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <ul class="nav nav-tabs tight-bottom">
      <li class="active"><a href="#search-users-tab" data-toggle="tab">Search Users</a></li>
      <li><a href="#create-users-tab" data-toggle="tab">Create Users</a></li>
      <li><a href="#links-tab" data-toggle="tab">Links</a></li>
    </ul>
  </div>

  <div class="modal-body">
    <div id="dialog-local-alerts"></div>

    <div class="tab-content">
      <div class="tab-pane active" id="search-users-tab">
        <%= render 'admin/base/users' %>
      </div>
      <div class="tab-pane" id="create-users-tab">
        <%= render 'create_users' %>
      </div>
      <div class="tab-pane" id="links-tab">
        <%= render 'links' %>
      </div>
    </div>    
  </div>

<% end %>
