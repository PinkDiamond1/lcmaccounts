<% content_for :javascript do %>
  <script type='text/javascript'>
    $(document).ready(function() {
      $('pre.code, pre.prettyprint').each(function(index) {
        $(this).html(Accounts.Ui.syntaxHighlight($(this).html()));
      });
    });
  </script>
<% end %>

<% content_for :application_body do %>

  <div id="application-body-api-docs">
    <%= yield %>
  </div>

<% end %>

<%= render :template => "layouts/application" %>
